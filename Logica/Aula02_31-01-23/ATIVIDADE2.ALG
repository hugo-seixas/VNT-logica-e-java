Algoritmo "atividade2"

Var

base, altura, area : real

Inicio

escreva("Digite a base do triângulo: ")
leia (base)
escreva("Digite a altura do triângulo: ")
leia (altura)

area <- base * altura

escreval("Base: ",  base)
escreval("Altura: ",  altura)
escreval("Área: ",  area)


Fimalgoritmo


Fimalgoritmo